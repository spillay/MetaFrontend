@model List<Nop.Plugin.PW.MetaDataModel.Models.SourceTablesSAPSystemModel>
@{
    Layout = "";
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <h4>Source Tables</h4>
    <hr />
    <table cellpadding="0" cellspacing="0">
        <tr>
            <th>ID</th>
            <th>Name</th>
        </tr>
        @foreach (SourceTablesSAPSystemModel table in Model)
        {
            <tr>
                <td>@table.ID</td>
                <td>@table.Name</td>
            </tr>
        }
    </table>
</body>
</html>